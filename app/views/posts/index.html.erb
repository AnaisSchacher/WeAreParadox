<div class="page-container">
  <% @posts.each do |post| %>
  <div class="post-card">
    <%= post.title %>
    <%= post.category %>
    <%= post.url %>
<iframe width="560" height="315" src="https://www.youtube.com/embed/<%= post.url %>" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>  </div>
  <% end %>
</div>
